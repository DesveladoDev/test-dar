ort((a,b) => b.category - a.category);\n        } catch(error) {\n          this.loading = false;\n          this.error = true;\n        } finally {\n          this.loading = false;\n       }\n    }\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}