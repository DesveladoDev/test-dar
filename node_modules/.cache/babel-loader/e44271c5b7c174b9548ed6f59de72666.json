{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'ListProducts',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      products: {},\n      loading: false,\n      error: false\n    };\n  },\n  async created() {\n    await this.listProducts();\n  },\n  methods: {\n    async listProducts() {\n      this.loading = true;\n      try {\n        const productsAPi = await axios.get('https://fakestoreapi.com/products sss');\n        let dataProduct = productsAPi.data;\n        this.products = dataProduct.map(function (element) {\n          return {\n            id: element.id,\n            title: element.title,\n            price: element.price,\n            category: element.category\n          };\n        }).sort((a, b) => b.category - a.category);\n      } catch (error) {\n        this.loading = false;\n        this.error = true;\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACJC,QAAQ,EAAE,CAAC,CAAC;MACZC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE;IACV;EACJ,CAAC;EACD,MAAMC,OAAO,GAAG;IACZ,MAAM,IAAI,CAACC,YAAY,EAAE;EAC7B,CAAC;EACDC,OAAO,EAAE;IACR,MAAMD,YAAY,GAAG;MAChB,IAAI,CAACH,OAAM,GAAI,IAAI;MACnB,IAAI;QACF,MAAMK,WAAU,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,uCAAuC,CAAC;QAC5E,IAAIC,WAAU,GAAIF,WAAW,CAACP,IAAI;QAClC,IAAI,CAACC,QAAO,GAAIQ,WAAW,CAACC,GAAG,CAAC,UAASC,OAAO,EAAC;UAC/C,OAAO;YACHC,EAAE,EAAED,OAAO,CAACC,EAAE;YACdC,KAAK,EAAEF,OAAO,CAACE,KAAK;YACpBC,KAAK,EAAEH,OAAO,CAACG,KAAK;YACpBC,QAAQ,EAAEJ,OAAO,CAACI;UACtB;QACJ,CAAC,CAAC,CAACC,IAAI,CAAC,CAACC,CAAC,EAACC,CAAC,KAAKA,CAAC,CAACH,QAAO,GAAIE,CAAC,CAACF,QAAQ,CAAC;MACzC,EAAE,OAAMZ,KAAK,EAAE;QACb,IAAI,CAACD,OAAM,GAAI,KAAK;QACpB,IAAI,CAACC,KAAI,GAAI,IAAI;MACnB,UAAU;QACR,IAAI,CAACD,OAAM,GAAI,KAAK;MACvB;IACH;EACF;AACF","names":["axios","name","props","msg","String","data","products","loading","error","created","listProd